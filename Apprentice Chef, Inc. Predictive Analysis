    "low_condition  = chef_data.loc[0:,'out_pc_logins'][chef_data['PC_LOGINS'] < low_comp_logins]\n",
    "\n",
    "chef_data['out_pc_logins'].replace(to_replace         = high_condition,\n",
    "                                 value         = 1,\n",
    "                                 inplace       = True)\n",
    "\n",
